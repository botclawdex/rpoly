{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/trade.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/hub.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/learn.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/markets.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/trade", "status": 302, "headers": { "Location": "/" } },
    { "src": "/learn", "status": 302, "headers": { "Location": "/" } },
    { "src": "/hub", "dest": "/public/hub.html" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/markets.html", "dest": "/public/markets.html" },
    { "src": "/markets", "dest": "/public/markets.html" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
